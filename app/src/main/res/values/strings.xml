<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">Question Time!</string>

    <string name="topics_options">Choose Topics</string>

    <string name="profile_options">Profile</string>

    <string name="question_header">
        Question <xliff:g id="q_number" example="1">%s</xliff:g>:
    </string>

    <string name="topics_title">Topics Selection</string>

    <string-array name="question_topics">
        <item>Sports</item>
        <item>History</item>
        <item>Geography</item>
        <item>Arts</item>
        <item>General Knowledge</item>
        <item>Music</item>
        <item>Science</item>
        <item>Animals</item>
        <item>Food and Drink</item>
    </string-array>

    <string name="section_number">
        <xliff:g id="sec_num" example="2">%s</xliff:g>/
        <xliff:g id="sec_tot" example="10">%s</xliff:g>
    </string>

    <string-array name="questions">
        <item>What is the capital of Namibia?</item>
        <item>Who wrote the novel 1984?</item>
        <item>Who is the current president of Ireland?</item>
        <item>Which English football team has won the most FA Cup trophies?</item>
        <item>What is the largest animal in the world?</item>
    </string-array>


    <string-array name="answers">
        <item>Windhoek</item>
        <item>George Orwell</item>
        <item>Michael D. Higgins</item>
        <item>Arsenal</item>
        <item>Blue Whale</item>
    </string-array>

    <string-array name="answer1_options">
        <item>Windhoek</item>
        <item>Namibia City</item>
        <item>Cairo</item>
        <item>Addis Ababa</item>
    </string-array>

    <string-array name="answer2_options">
        <item>John Steinbeck</item>
        <item>George Orwell</item>
        <item>Ernest Hemingway</item>
        <item>Mark Twain</item>
    </string-array>

    <string-array name="answer3_options">
        <item>Mary McAleese</item>
        <item>David Cameron</item>
        <item>Mary Robinson</item>
        <item>Michael D. Higgins</item>
    </string-array>

    <string-array name="answer4_options">
        <item>Manchester United</item>
        <item>Arsenal</item>
        <item>Liverpool</item>
        <item>Chelsea</item>
    </string-array>

    <string-array name="answer5_options">
        <item>Indian Elephant</item>
        <item>Blue Whale</item>
        <item>Hippopotamus</item>
        <item>Rhinoceros</item>
    </string-array>

    <array name="options">
        <item>@array/answer1_options</item>
        <item>@array/answer2_options</item>
        <item>@array/answer3_options</item>
        <item>@array/answer4_options</item>
        <item>@array/answer5_options</item>
    </array>

</resources>
